{
  "name": "epoch-seconds",
  "version": "4.0.1",
  "type": "module",
  "description": "Returns the amount of seconds passed since Unix epoch (1970 UTC).",
  "keywords": [
    "1970",
    "date",
    "date-now",
    "epoch",
    "gmt",
    "linux",
    "millis",
    "milliseconds",
    "now",
    "posix",
    "posix-epoch",
    "second",
    "seconds",
    "time",
    "timestamp",
    "unix",
    "unix-epoch",
    "utc"
  ],
  "author": "Jaid <jaid.jsx@gmail.com> (https://github.com/Jaid)",
  "homepage": "https://github.com/jaid/epoch-seconds",
  "funding": "https://github.com/sponsors/jaid",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaid/epoch-seconds"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "wireit",
    "build:production": "wireit",
    "test": "wireit",
    "test-build": "wireit",
    "test-build:production": "wireit",
    "release": "wireit",
    "release:windows": "wireit"
  },
  "wireit": {
    "test": {
      "env": {
        "NODE_ENV": "test",
        "MAIN_MODULE": "src/index.ts"
      },
      "files": [
        "src/**/*",
        "lib/**/*",
        "test/**/*",
        "package.json"
      ],
      "command": "node --import tsx test/test.ts"
    },
    "test-build": {
      "env": {
        "NODE_ENV": "test",
        "MAIN_MODULE": "./dist/package/epoch-seconds/development/lib.js"
      },
      "dependencies": [
        "build"
      ],
      "command": "node --import tsx test/test.ts"
    },
    "test-build:production": {
      "env": {
        "NODE_ENV": "test",
        "MAIN_MODULE": "./dist/package/epoch-seconds/production/lib.js"
      },
      "dependencies": [
        "build:production"
      ],
      "command": "node --import tsx test/test.ts"
    },
    "build:production": {
      "env": {
        "NODE_ENV": "production"
      },
      "output": [
        "dist/package/epoch-seconds/production"
      ],
      "files": [
        "src/**/*",
        "lib/**/*",
        "rollup.config.js",
        "package.json"
      ],
      "command": "node node_modules/rollup/dist/bin/rollup --config"
    },
    "build": {
      "env": {
        "NODE_ENV": "development"
      },
      "output": [
        "dist/package/epoch-seconds/development"
      ],
      "files": [
        "src/**/*",
        "lib/**/*",
        "rollup.config.js",
        "package.json"
      ],
      "command": "node node_modules/rollup/dist/bin/rollup --config"
    },
    "release": {
      "dependencies": [
        "test-build:production",
        "build:production"
      ],
      "command": "fd --unrestricted --full-path 'dist/package/.+/production/package.json' --exec bash -c 'npm publish {//}'"
    },
    "release:windows": {
      "dependencies": [
        "test-build:production",
        "build:production"
      ],
      "command": "pwsh -Command \"Get-ChildItem -Path 'dist/package/*/production/package.json' | ForEach-Object { npm publish $_.DirectoryName }\""
    }
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@types/node": "^20.12.4",
    "browserslist-config-jaid-universal": "^3.0.0",
    "bundle-declarations-webpack-plugin": "^5.1.1",
    "eslint-config-jaid": "^8.24.0",
    "rollup-config-factory": "^0.3.0",
    "tsconfig-jaid-node": "^8.1.0",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tsx": "^4.7.2",
    "type-fest": "^4.15.0",
    "typescript": "^5.4.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "wireit": "^0.14.4",
    "zeug": "^0.4.0"
  }
}